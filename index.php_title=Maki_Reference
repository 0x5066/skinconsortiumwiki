</li><li> <i>extern double System.<b>ln</b>(double value);</i>
</li></ul>
<a name="Saving"></a><h2><span class="editsection">[<a href="/index.php?title=Maki_Reference&amp;action=edit&amp;section=20" title="Edit section: Saving">edit</a>]</span> <span class="mw-headline"> <a href="/index.php?title=Maki-Saving&amp;action=edit" class="new" title="Maki-Saving">Saving</a> </span></h2>
<ul><li> <i>extern System.<b>setPrivateString</b>(string section, string item, string value);</i>
</li><li> <i>extern System.<b>setPrivateInt</b>(string section, string item, int value);</i>
</li><li> <i>extern String System.<b>getPrivateString</b>(String section, String item, String defvalue);</i>
</li><li> <i>extern Int System.<b>getPrivateInt</b>(String section, String item, Int defvalue);</i>
</li><li> <i>extern System.<b>setPublicString</b>(String item, String value);</i>
</li><li> <i>extern System.<b>setPublicInt</b>(String item, Int value);</i>
</li><li> <i>extern String System.<b>getPublicString</b>(String item, String defvalue);</i>
</li><li> <i>extern Int System.<b>getPublicInt</b>(String item, Int defvalue);</i>
</li></ul>
